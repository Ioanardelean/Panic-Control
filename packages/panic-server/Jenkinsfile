pipeline {
  agent any
    
    
  stages {
        
    stage('Git') {
      steps {
        git 'https://github.com/Ioanardelean/Panic-server.git'
      }
    }
     
    stage('Build') {
      steps {
        sh 'npm install'
         sh '<<Build Command>>'
      }
    }  
    
            
    stage('Test') {
      steps {
        sh 'npm test'
      }
    }
  }
}